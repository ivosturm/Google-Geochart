<?xml version="1.0" encoding="utf-8" ?>
<widget id="GeoMap.widget.GeoMap" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Google GeoMap</name>
    <description>Google GeoMap</description>
    <icon>


    </icon>
    <properties>

        <property key="mapEntity" type="entity" required="true">
            <caption>Objects</caption>
            <category>Data Source</category>
            <description>The objects to show on the map.</description>
        </property>
        <property key="xpathConstraint" type="entityConstraint" entityProperty="mapEntity" required="false">
            <caption>XPath Constraint</caption>
            <category>Data Source</category>
            <description>The XPath constraint on the objects.</description> 
        </property>
        <property key="mapWidth" type="integer" defaultValue="350">
            <caption>Width</caption>
            <category>Map</category>
            <description>The map's width in px.</description> 
        </property>
	<property key="dataType" type="enumeration" required="true" defaultValue="region">
            <caption>Data Type</caption>
            <category>Map</category>
            <description>Determines which Google Chart to display. </description>
			<enumerationValues>
				<enumerationValue key="region">Region</enumerationValue>
				<enumerationValue key="markers">Markers</enumerationValue>
				<enumerationValue key="auto">Auto</enumerationValue>
				<enumerationValue key="text">Text</enumerationValue>
			</enumerationValues>
	</property>
	<property key="onClickMicroflow" type="microflow" entityProperty="mapEntity" required="false"  >
			<caption>On Click Microflow</caption>
			<category>Map</category>
			<description>Select the Microflow that is fired on click of an area.</description>
			<returnType type="Void" />
		</property>
		<property key="mapBorderColor" type="string" defaultValue="#D12148">
            <caption>Border Color</caption>
            <category>Styling</category>
            <description>The color of the border of the mapobject</description> 
        </property>
		<property key="mapBorderRadius" type="integer" defaultValue="4">
            <caption>Border Radius</caption>
            <category>Styling</category>
            <description>The radius (in pixel) for the border of the map</description> 
        </property>
		<property key="backGroundColor" type="string" defaultValue="#D5E8EE">				
            <caption>BackGround Color</caption>
            <category>Styling</category>
            <description>The color of the sea in the map</description> 
        </property>
		<property key="colorAxisStart" type="string" defaultValue="#00853f">				
            <caption>ColorAxis Start</caption>
            <category>Styling</category>
            <description>The beginning color for the scale of the color axis.</description> 
        </property>
		<property key="colorAxisEnd" type="string" defaultValue="#e31b23">		
            <caption>ColorAxis End</caption>
            <category>Styling</category>
            <description>The ending color for the scale of the color axis.</description> 
        </property>
		<property key="toolTipColor" type="string" defaultValue="#FF0000">			
            <caption>ToolTip Color</caption>
            <category>Styling</category>
            <description>The color for the appearing tooltip text.</description> 
        </property>
        <property key="showZoomOut" type="boolean" defaultValue="true">
            <caption>Show Zoom Out</caption>
            <category>Map</category>
            <description>If true, display a button with the label specified by the zoomOutLabel property. Note that this button does nothing when clicked, except throw the zoomOut event.</description>
        </property>
		<property key="zoomOutLabel" type="string" defaultValue="Zoom Out">
            <caption>Zoom Out Label</caption>
            <category>Map</category>
            <description>Label for the zoom button.</description> 
        </property>
	<property key="region" type="string" defaultValue="world">
            <caption>Region</caption>
            <category>Map</category>
            <description>The area to display on the map. (Surrounding areas will be displayed as well.) Can be either a country code (in uppercase ISO-3166 format), or a one of the following strings. world - (Whole world)
us_metro - (United States, metro areas)
005 - (South America)
013 - (Central America)
021 - (North America)
002 - (All of Africa)
017 - (Central Africa)
015 - (Northern Africa)
018 - (Southern Africa)
030 - (Eastern Asia)
034 - (Southern Asia)
035 - (Asia/Pacific region)
009 - (Oceania)
145 - (Middle East)
143 - (Central Asia)
151 - (Northern Asia)
154 - (Northern Europe)
155 - (Western Europe)
039 - (Southern Europe)</description> 
        </property>
		<property key="showLegend" type="boolean" defaultValue="true">
            <caption>Enable Legend</caption>
            <category>Legend</category>
            <description>	If true, display a legend for the map.</description> 
        </property>
		<property key="legendName" type="string" defaultValue="Population">
            <caption>Name</caption>
            <category>Legend</category>
            <description>The name for the legend</description> 
        </property>
		<property key="legendFontColor" type="string" defaultValue="#000000">
            <caption>Font Color</caption>
            <category>Legend</category>
            <description>The color of the font of the legend</description> 
        </property>
		<property key="legendFontSize" type="integer" defaultValue="14">
            <caption>Font Size</caption>
            <category>Legend</category>
            <description>The size of the font of the legend</description> 
        </property>
        <property key="apiAccessKey" type="string" required="false">
            <caption>Maps API Access Key</caption>
            <category>Google API</category>
            <description>The Google Maps JavaScript API v3 does not require an API key to function correctly. However, Google strongly encourages you to load the Maps API using an APIs Console key which allows you to monitor your application's Maps API usage.</description>
        </property>
        <property key="countryAttr" type="attribute" entityProperty="mapEntity" required="true" isPath="optional" pathType="reference">
            <caption>Country attribute</caption>
            <category>Data Source</category>
            <description>The country attribute.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="numberAttr" type="attribute" entityProperty="mapEntity" required="true" isPath="optional" pathType="reference">
            <caption>Number attribute</caption>
            <category>Data Source</category>
            <description>The number attribute.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
                <attributeType name="Float"/>
            </attributeTypes>
        </property> 
		<property key="enableLogging" type="boolean" defaultValue="false">
		  <caption>Enable Logging</caption>
		  <category>Logging</category>
		  <description>The boolean can enable/disable logging on this widget for debugging purposes.</description>
		</property> 
    </properties>
</widget>
